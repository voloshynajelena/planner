{
  "name": "planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node index.js",
    "server": "pm2 start index.js && pm2 monit",
    "server:start": "pm2 start index.js",
    "server:restart": "pm2 restart all",
    "server:stop": "pm2 stop all",
    "server:monitor": "pm2 monit",
    "deploy": "npm run build && git add . && git commit -m 'automessage' && git push origin master"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "pm2": "^3.0.0",
    "vue": "^2.5.16",
    "vue-axios": "^2.1.2",
    "vuedraggable": "^2.16.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "vue-template-compiler": "^2.5.16",
    "request": "^2.81.0",
    "tape": "^4.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
